			<button id="small-dogs" class="btn waves-effect waves-light" type="submit" name="action">Small Dogs<i class="small material-icons right">pets</i></button>
			<button id="medium-dogs" class="btn waves-effect waves-light" type="submit" name="action">Medium Dogs<i class="medium material-icons right">pets</i></button>
			<button id="large-dogs" class="btn waves-effect waves-light" type="submit" name="action">Large Dogs<i class="large material-icons right">pets</i></button>
		</div>

		<div id="search-by-activity">
			<h4 class="option">Search By Activity Level</h4>
			<button id="sedentary-dogs" class="btn waves-effect waves-light" type="submit" name="action">Sedentary Dogs<i class="material-icons right">hotel</i></button>
			<button id="moderately-energetic-dogs" class="btn waves-effect waves-light" type="submit" name="action">Moderately-Energetic Dogs<i class="material-icons right">directions_walk</i></button>
			<button id="hyperactive-dogs" class="btn waves-effect waves-light" type="submit" name="action">Hyperactive Dogs<i class="material-icons right">directions_run</i></button>
